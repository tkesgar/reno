{
  "name": "@tkesgar/reno",
  "version": "0.2.0",
  "description": "Library to help with Node.js server development",
  "keywords": [
    "env",
    "utility"
  ],
  "license": "MIT",
  "author": "Ted Kesgar <t.kesgar@live.com> (https://tkesgar.com)",
  "contributors": [
    "Ted Kesgar <t.kesgar@live.com> (https://tkesgar.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkesgar/reno.git"
  },
  "bugs": {
    "url": "https://github.com/tkesgar/reno/issues"
  },
  "homepage": "https://github.com/tkesgar/reno#readme",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist/**"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "tsc -w",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --write '**/*'",
    "test": "jest",
    "clean": "rm -rf dist/**",
    "build": "tsc",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prepare": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.9.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "~2.0.2",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "dotenv": "^8.2.0"
  }
}
