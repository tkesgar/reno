{
  "name": "@tkesgar/reno",
  "version": "0.2.1",
  "description": "Library to help with Node.js server development",
  "keywords": [
    "env",
    "utility"
  ],
  "license": "MIT",
  "author": "Ted Kesgar <t.kesgar@live.com> (https://tkesgar.com)",
  "contributors": [
    "Ted Kesgar <t.kesgar@live.com> (https://tkesgar.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkesgar/reno.git"
  },
  "bugs": {
    "url": "https://github.com/tkesgar/reno/issues"
  },
  "homepage": "https://github.com/tkesgar/reno#readme",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist/**"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "tsc -w",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --write '**/*'",
    "test": "jest",
    "clean": "rm -rf dist/**",
    "build": "tsc",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prepare": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "conventional-changelog-cli": "^2.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "prettier": "~2.1.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0"
  }
}
